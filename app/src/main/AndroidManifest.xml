<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.simplerel">

    <application
        android:name="io.github.jokoframework.singleton.SimpleApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/mboehao"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="io.github.jokoframework.activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="io.github.jokoframework.activity.ShowActivity"
            android:label="@string/app_name"
            android:windowSoftInputMode="stateHidden" />

        <meta-data
            android:name="com.parse.SERVER_URL"
            android:value="@string/parse_server_url" />
        <meta-data
            android:name="com.parse.APPLICATION_ID"
            android:value="@string/parse_app_id" />
    </application>

</manifest>